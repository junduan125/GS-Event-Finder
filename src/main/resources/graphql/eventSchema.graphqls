extend type Mutation {
	createEvent(eventTime: Int!, eventType: Int!): GSEvent
	joinEvent(id: ID): GSEvent
	leaveEvent(id: ID): Boolean
}

type GSEvent implements Node {
	id: ID!
	eventTime: Int
	eventType: Int
}

type GSEventEdgeConnection {
  edges: [GSEventEdge]
  pageInfo: PageInfo!
}

type GSEventEdge {
	cursor: String!
	node: GSEvent
}